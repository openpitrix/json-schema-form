{
  "name": "json-schema-form",
  "version": "0.0.1",
  "description": "Form generator for qingcloud customized json-schema",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rimraf build && webpack --config webpack.dev.js --progress",
    "dev": "webpack-dev-server --config webpack.dev.js --hot-only --inline",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,md}": [
